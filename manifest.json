        "buscarMovimentacoes": {
            "script": "dist/buscarMovimentacoes.js",
            "description": "Busca movimentações de um processo pelo número CNJ",
            "category": "tribunais",
            "params": [
                {
                    "name": "numeroProcesso",
                    "type": "string",
                    "required": true,
                    "description": "Número do processo no formato CNJ",
                    "form": {
                        "type": "text",
                        "label": "Número do Processo",
                        "placeholder": "Ex: 1234567-89.2023.8.26.0001",
                        "help": "Digite o número do processo no formato CNJ (NNNNNNN-DD.AAAA.J.TR.OOOO)"
                    }
                }
            ]
        },
        "secrets": {
            "name": "escavadorToken",
            "type": "string",
