{
    "name": "escavador",
    "version": "2.0.0",
    "description": "Aplicação para consulta de processos jurídicos via API Escavador",
    "functions": {
        "buscarProcesso": {
            "script": "dist/buscarProcessoCNJ.js",
            "description": "Busca um processo pelo número CNJ",
            "category": "tribunais",
            "params": [
                {
                    "name": "numeroProcesso",
                    "type": "string",
                    "required": true,
                    "description": "Número do processo no formato CNJ",
                    "form": {
                        "type": "text",
                        "label": "Número do Processo",
                        "placeholder": "Ex: 1234567-89.2023.8.26.0001",
                        "help": "Digite o número do processo no formato CNJ (NNNNNNN-DD.AAAA.J.TR.OOOO)"
                    }
                }
            ],
            "secrets": {
                "name": "escavadorToken",
                "type": "string",
                "required": true,
                "description": "Token de acesso à API do Escavador em base64 (ex.: {\"escavadorToken\":\"seu_token_aqui\"} codificado em base64)",
                "form": {
                    "type": "text",
                    "label": "Token do Escavador",
                    "placeholder": "Insira seu token aqui",
                    "help": "Token necessário para autenticação na API do Escavador"
                }
            }
        },
        "buscarEnvolvido": {
            "script": "dist/buscarEnvolvido.js",
            "description": "Busca processos de um envolvido por nome ou CPF/CNPJ",
            "category": "tribunais",
            "params": [
                {
                    "name": "nome",
                    "type": "string",
                    "required": false,
                    "description": "Nome do envolvido (obrigatório se CPF/CNPJ não for informado)",
                    "form": {
                        "type": "text",
                        "label": "Nome do Envolvido",
                        "placeholder": "Ex: João da Silva",
                        "help": "Digite o nome completo da pessoa ou empresa"
                    }
                },
                {
                    "name": "cpfCnpj",
                    "type": "string",
                    "required": false,
                    "description": "CPF ou CNPJ do envolvido (obrigatório se nome não for informado)",
                    "form": {
                        "type": "text",
                        "label": "CPF/CNPJ",
                        "placeholder": "Ex: 123.456.789-00 ou 12.345.678/0001-90",
                        "help": "Digite o CPF (pessoa física) ou CNPJ (pessoa jurídica)"
                    }
                },
                {
                    "name": "limit",
                    "type": "number",
                    "required": false,
                    "description": "Quantidade de processos por página",
                    "defaultValue": 50,
                    "form": {
                        "type": "select",
                        "label": "Limite por página",
                        "placeholder": "Selecione o limite",
                        "help": "Quantidade de processos retornados por página",
                        "defaultValue": 50,
                        "options": [
                            {
                                "label": "50 processos",
                                "value": 50
                            },
                            {
                                "label": "100 processos",
                                "value": 100
                            }
                        ]
                    }
                },
                {
                    "name": "status",
                    "type": "string",
                    "required": false,
                    "description": "Filtrar por status do processo",
                    "defaultValue": "ATIVO",
                    "form": {
                        "type": "select",
                        "label": "Status do Processo",
                        "placeholder": "Selecione o status",
                        "help": "Filtrar processos por status (classificação por IA)",
                        "defaultValue": "ATIVO",
                        "options": [
                            {
                                "label": "Ativo",
                                "value": "ATIVO"
                            },
                            {
                                "label": "Inativo",
                                "value": "INATIVO"
                            }
                        ]
                    }
                },
                {
                    "name": "dataMinima",
                    "type": "string",
                    "required": false,
                    "description": "Data mínima de início do processo",
                    "form": {
                        "type": "text",
                        "label": "Data Mínima",
                        "placeholder": "YYYY-MM-DD",
                        "help": "Filtrar processos que iniciaram após esta data (formato: YYYY-MM-DD)"
                    }
                },
                {
                    "name": "dataMaxima",
                    "type": "string",
                    "required": false,
                    "description": "Data máxima de início do processo",
                    "form": {
                        "type": "text",
                        "label": "Data Máxima",
                        "placeholder": "YYYY-MM-DD",
                        "help": "Filtrar processos que iniciaram antes desta data (formato: YYYY-MM-DD)"
                    }
                }
            ],
            "secrets": {
                "name": "escavadorToken",
                "type": "string",
                "required": true,
                "description": "Token de acesso à API do Escavador em base64 (ex.: {\"escavadorToken\":\"seu_token_aqui\"} codificado em base64)",
                "form": {
                    "type": "text",
                    "label": "Token do Escavador",
                    "placeholder": "Insira seu token aqui",
                    "help": "Token necessário para autenticação na API do Escavador"
                }
            }
        },
